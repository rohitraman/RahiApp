<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.roshan.rahiapp">

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LoginActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".RegisterActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SplashScreenActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".PinActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".GetAvailabilityActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".GetPNRActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".GetRouteActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".GetAllTrainsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".GetTrainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".GetFareActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".GetTrainsOnActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".GetStatusActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".splash_rahi"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".RouteViewActivity" />
        <activity android:name=".from_to" />
        <activity
            android:name=".TimeLineActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ForgotPasswordActivity" />
        <activity android:name=".ChangePasswordActivity"></activity>
    </application>

</manifest>